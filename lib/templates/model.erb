class <%= model_name %> < <%= base_class %>
  self.table_name = <%= name.inspect %>
  self.primary_key = :<%= primary_key %>

<% associations.each do |association| %>
  <%= association.macro %> :<%= association.class_name.underscore %>, class_name: :<%= association.class_name %>, foreign_key: :<%= association.foreign_key %>, primary_key: :<%= association.primary_key %>
<% end %>

<% appends.each do |append| %>
  <%= append %>
<% end %>
end
